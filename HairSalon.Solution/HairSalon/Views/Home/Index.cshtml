@{
  Layout = "_Layout";
}

<h1>Stylists</h1>

@if (Model.Count == 0)
{
  <h3>No stylists are employed yet</h3>
}

@foreach (var stylist in Model)
{
  <h3 id="stylists"><a href="/stylist/@stylist.Id">@stylist.Name</a></h3>
}
<h2 class="waves-effect waves-light btn"><a href="/stylist">Add New Stylist</a></h2>
